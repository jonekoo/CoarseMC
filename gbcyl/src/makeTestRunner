
# makefile to compile TestRunner.f90
#
# funit generated this file on Wed Feb 11 16:42:55 +0200 2009.

OBJ=nrtype.o gayberne.o gayberne_fun.o TestRunner.o

all:testrunner

testrunner: $(OBJ)
	gfortran -Wall -std=f95 -pedantic -o TestRunner $(OBJ)

gayberne.o: gayberne.f90 nrtype.o
	(cd .; gfortran -Wall -std=f95 -pedantic  -c gayberne.f90)
gayberne_fun.o: gayberne_fun.f90 gayberne.o nrtype.o
	(cd .; gfortran -Wall -std=f95 -pedantic  -c gayberne_fun.f90)
nrtype.o: nrtype.f90 
	(cd .; gfortran -Wall -std=f95 -pedantic  -c nrtype.f90)
TestRunner.o: TestRunner.f90 gayberne_fun.o
	(cd .; gfortran -Wall -std=f95 -pedantic  -c TestRunner.f90)
  
